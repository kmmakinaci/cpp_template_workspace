{
    "version": 3,
    "configurePresets": [
      {
        "name": "debug",
        "hidden": false,
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build/debug",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Debug",
          "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
        }
      },
      {
        "name": "release",
        "hidden": false,
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build/release",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Release",
          "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
        }
      }
    ]
  }
  